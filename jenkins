pipeline
{
    agent any
    stages
    {
        stage('Continuos Download')
        {
            steps
	    {

                        git 'https://github.com/chintakiran/maven.git'
            }
        }


	 stage('Continuos Build')
        {
            steps
            {
                sh 'mvn package'
            }
        }





	}
}
